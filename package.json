  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "files": [
    "lib/index.js",
    "lib/index.d.ts"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/project-nos/eslint-config.git"
